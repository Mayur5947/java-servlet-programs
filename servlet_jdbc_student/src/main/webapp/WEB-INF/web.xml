<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<context-param>
		<param-name>school</param-name>
		<param-value>raisoni</param-value>
	</context-param>
	<servlet>
		<servlet-name>raj</servlet-name>
		<servlet-class>servlet_jdbc_student.StudentController</servlet-class>
		<init-param>
			<param-name>os</param-name>
			<param-value>100000</param-value>
		</init-param>
		<init-param>
			<param-name>I</param-name>
			<param-value>50000</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>raj</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>mayur</servlet-name>
		<servlet-class>servlet_jdbc_student.LoginController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>mayur</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>


</web-app>
